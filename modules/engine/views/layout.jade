    body
        div
            // BEGIN BACK TO TOP
            a#totop(href='#')
                i.fa.fa-angle-up
            // END BACK TO TOP
            // BEGIN TOPBAR
            #header-topbar-option-demo.page-header-topbar
                nav#topbar.navbar.navbar-default.navbar-static-top(role='navigation', style='margin-bottom: 0; z-index: 2;')
                    .navbar-header
                        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.sidebar-collapse')
                            span.sr-only Toggle navigation
                            span.icon-bar
                            span.icon-bar
                            span.icon-bar
                        a#logo.navbar-brand(href='/')
                            span.fa.fa-rocket
                            span.logo-text CMP
                            span.logo-text-icon(style='display: none') F
                    .topbar-main
                        a#menu-toggle.hidden-xs(href='#')
                            i.fa.fa-bars
                        ul.nav.navbar-nav
                            li.active
                                a(href, ui-sref="claims") Claims
                        form#topbar-search.hidden-xs(action='#', method='GET')
                            .input-group
                                input.form-control(type='text', placeholder='Search...')
                                span.input-group-btn
                                    a.btn.submit(href='javascript:;')
                                        i.fa.fa-search
                        ul.nav.navbar.navbar-top-links.navbar-right.mbn
                            li.dropdown.topbar-user
                                a.dropdown-toggle(data-hover='dropdown', href='#')
                                    img.img-responsive.img-circle(src='/images/me.jpg', alt='')
                                    span.hidden-xs Carl Ngan
                                    span.caret
                                ul.dropdown-menu.dropdown-user.pull-right
                                    li
                                        a(href, ui-sref="logout")
                                            i.fa.fa-key
                                            | Log Out
            // END TOPBAR
            #wrapper
                // BEGIN SIDEBAR MENU
                nav#sidebar.navbar-default.navbar-static-side(role='navigation')
                    .sidebar-collapse.menu-scroll
                        ul#side-menu.nav
                            li.user-panel
                                .thumb
                                    img.img-circle(src='/images/me.jpg', alt='')
                                .info
                                    p Carl Ngan
                                    ul.list-inline.list-unstyled
                                        li
                                            a(href, ui-sref="logout" data-hover='tooltip', title='Logout')
                                                i.fa.fa-sign-out
                                .clearfix
                            li.active
                                a(href, ui-sref="claims")
                                    i.fa.fa-tachometer.fa-fw
                                        .icon-bg.bg-orange
                                    span.menu-title Claims
                // END SIDEBAR MENU
                // BEGIN PAGE WRAPPER
                #page-wrapper
                    ui-view
                // BEGIN FOOTER
                #footer
                    .copyright 2016 Â© Carl Ngan - Interview
                // END FOOTER
                // END PAGE WRAPPER

